{
  "name": "nq-connector",
  "version": "0.4.20",
  "author": "NQ Framework",
  "description": "A client application for managing nq framework and connecting to the on premise database",
  "main": "dist/electron/main",
  "license": "MIT",
  "files": [
    "dist",
    "*.js"
  ],
  "scripts": {
    "ng": "ng",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "build": "npm run build:ui && npm run build:electron",
    "build:ui": "ng build --base-href ./",
    "build:electron": "tsc -p ./electron/tsconfig.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "ng test",
    "test:ci": "ng test --no-watch --no-progress --browsers=ChromeHeadless",
    "lint": "npm run lint:ui && npm run lint:electron",
    "lint:ui": "ng lint",
    "lint:electron": "tslint -p ./electron/tsconfig.json -c ./tslint.json",
    "e2e": "ng e2e",
    "prepare": "npm run build",
    "prepublishOnly": "npm test:ci && npm run lint",
    "preversion": "npm run lint && npm run test:ci && npm run build:ui",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "locale": "npm run build && npm run locale:extract",
    "locale:extract": "locl extract -s './dist/nq-connector/*.js' -o './rs.json'"
  },
  "private": true,
  "config": {
    "forge": {
      "electronPackagerConfig": {
        "asar": "true"
      },
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "nq-connector"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "dependencies": {
    "@angular/animations": "~10.0.4",
    "@angular/cdk": "^9.2.4",
    "@angular/common": "~10.0.4",
    "@angular/compiler": "~10.0.4",
    "@angular/core": "~10.0.4",
    "@angular/fire": "^6.0.2",
    "@angular/forms": "~10.0.4",
    "@angular/localize": "^10.0.4",
    "@angular/platform-browser": "~10.0.4",
    "@angular/platform-browser-dynamic": "~10.0.4",
    "@angular/router": "~10.0.4",
    "@locl/core": "^1.0.0-rc.1",
    "@nqframework/models": "^1.2.11",
    "@teamhive/lottie-player": "^1.0.0",
    "angular-mocks": "^1.8.0",
    "bulma": "^0.8.2",
    "electron-squirrel-startup": "^1.0.0",
    "firebase": "^7.16.1",
    "ngx-electron": "^2.2.0",
    "primeflex": "^1.3.1",
    "primeicons": "^2.0.0",
    "primeng": "^9.1.3",
    "rxjs": "~6.5.5",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/architect": "~0.900",
    "@angular-devkit/build-angular": "~0.1000.3",
    "@angular/cli": "^10.0.3",
    "@angular/compiler-cli": "~10.0.4",
    "@angular/language-service": "~10.0.4",
    "@electron-forge/cli": "^6.0.0-beta.52",
    "@electron-forge/maker-deb": "6.0.0-beta.50",
    "@electron-forge/maker-rpm": "6.0.0-beta.50",
    "@electron-forge/maker-squirrel": "6.0.0-beta.50",
    "@electron-forge/maker-zip": "6.0.0-beta.50",
    "@locl/cli": "0.0.1-beta.9",
    "@types/jasmine": "^3.5.11",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.12.50",
    "codelyzer": "^5.1.2",
    "electron": "^13.6.6",
    "electron-packager": "^14.2.1",
    "firebase-tools": "^8.6.0",
    "fuzzy": "^0.1.3",
    "inquirer": "^6.2.2",
    "inquirer-autocomplete-prompt": "^1.0.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "prettier": "^2.0.5",
    "protractor": "~7.0.0",
    "puppeteer": "^3.3.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "^3.9.7"
  }
}
